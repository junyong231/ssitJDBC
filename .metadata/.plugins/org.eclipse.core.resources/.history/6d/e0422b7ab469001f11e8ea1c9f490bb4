 */

@Getter
@Setter
@ToString
@NoArgsConstructor
@AllArgsConstructor
public class EmpDeptSalGradeVO2 {
//	private DeptVO dvo;
//	private EmpVO evo;  이렇게 통쨰로 해도 되지만 불필요한 정보가..
	

	private int grade;
	private String lh;
	private int cg;

	@Override
	public String toString() {
	    return String.format("등급: %d, ( %s ), 인원수: %d", grade, lh, cg);
	}
	
	
}   //class







